### SCROLLBAR ###

style "calf-scrollbar"
{
  engine "pixmap"
  {
    # backgrounds
    image
    {
      function          = BOX
      recolorable       = FALSE
      detail            = "trough"
      file              = "background_black.png"
      stretch           = FALSE
      orientation       = HORIZONTAL
    }
    image
    {
      function          = BOX
      recolorable       = FALSE
      detail            = "trough"
      file              = "background_black.png"
      stretch           = TRUE
      orientation       = VERTICAL
    }
    # slider horizontal
    image
    {
      function          = SLIDER
      recolorable       = FALSE
      state             = NORMAL
      file              = "scroller-horiz.png" 
      border            = { 24,29,1,1 }
      stretch           = TRUE
      orientation       = HORIZONTAL
      overlay_file      = "led_normal.png"
      overlay_stretch   = FALSE
      orientation       = HORIZONTAL
    }
    image
    {
      function          = SLIDER
      recolorable       = FALSE
      state             = ACTIVE
      shadow            = IN
      file              = "scroller-horiz.png" 
      border            = { 24,29,1,1 }
      stretch           = TRUE
      orientation       = HORIZONTAL
      overlay_file      = "led_active.png"
      overlay_stretch   = FALSE
      orientation       = HORIZONTAL
    }
    image
    {
      function          = SLIDER
      recolorable       = FALSE
      state             = PRELIGHT
      file              = "scroller-horiz-prelight.png" 
      border            = { 24,29,1,1 }
      stretch           = TRUE
      orientation       = HORIZONTAL
      overlay_file      = "led_prelight.png"
      overlay_stretch   = FALSE
      orientation       = HORIZONTAL
    }
    image
    {
      function          = SLIDER
      recolorable       = FALSE
      state             = INSENSITIVE
      file              = "scroller-horiz-insens.png"
      border            = { 24,29,1,1 }
      stretch           = TRUE
      orientation       = HORIZONTAL
      overlay_file      = "led_inactive.png"
      overlay_stretch   = FALSE
      orientation       = HORIZONTAL
    }
    # slider vertical
    image
    {
      function          = SLIDER
      recolorable       = FALSE
      state             = NORMAL
      file              = "scroller-vert.png" 
      border            = { 1,1,24,29 }
      stretch           = TRUE
      orientation       = VERTICAL
      overlay_file      = "led2_normal.png"
      overlay_stretch   = FALSE
      orientation       = VERTICAL
    }
    image
    {
      function          = SLIDER
      recolorable       = FALSE
      state             = ACTIVE
      shadow            = IN
      file              = "scroller-vert.png" 
      border            = { 1,1,24,29 }
      stretch           = TRUE
      orientation       = VERTICAL
      overlay_file      = "led2_active.png"
      overlay_stretch   = FALSE
      orientation       = VERTICAL
    }
    image
    {
      function          = SLIDER
      recolorable       = FALSE
      state             = PRELIGHT
      file              = "scroller-vert-prelight.png" 
      border            = { 1,1,24,29} 
      stretch           = TRUE
      orientation       = VERTICAL
      overlay_file      = "led2_prelight.png"
      overlay_stretch   = FALSE
      orientation       = VERTICAL
    }
    image
    {
      function          = SLIDER
      recolorable       = FALSE
      state             = INSENSITIVE
      file              = "scroller-vert-insens.png"
      border            = { 1,1,24,29 }
      stretch           = TRUE
      orientation       = VERTICAL
      overlay_file      = "led2_inactive.png"
      overlay_stretch   = FALSE
      orientation       = VERTICAL
    }
    # buttons
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = NORMAL
      file              = "screw2.png"
      #border           = { 12, 2, 2, 9 }
      stretch           = TRUE
      arrow_direction   = UP
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = PRELIGHT
      file              = "screw2.png"
      #border           = { 12, 2, 2, 9 }
      stretch           = TRUE
      arrow_direction   = UP
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = ACTIVE
      file              = "screw2.png"
      #border           = { 12, 2, 2, 9 }
      stretch           = TRUE
      arrow_direction   = UP
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = INSENSITIVE
      file              = "screw2.png"
      #border           = { 12, 2, 2, 9 }
      stretch           = TRUE
      arrow_direction   = UP
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = NORMAL
      file              = "screw1.png"
      #border           = { 12, 2, 10, 2 }
      stretch           = TRUE
      arrow_direction   = DOWN
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = PRELIGHT
      file              = "screw1.png"
      #border           = { 12, 2, 10, 2 }
      stretch           = TRUE
      arrow_direction   = DOWN
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = ACTIVE
      file              = "screw1.png"
      #border           = { 12, 2, 10, 2 }
      stretch           = TRUE
      arrow_direction   = DOWN
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = INSENSITIVE
      file              = "screw1.png"
      #border           = { 12, 2, 10, 2 }
      stretch           = TRUE
      arrow_direction   = DOWN
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = NORMAL
      file              = "screw2.png"
      #border           = { 2, 9, 2, 13 }
      stretch           = TRUE
      arrow_direction   = RIGHT
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = PRELIGHT
      file              = "screw2.png"
      #border           = { 2, 9, 2, 13 }
      stretch           = TRUE
      arrow_direction   = RIGHT
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = ACTIVE
      file              = "screw2.png"
      #border           = { 2, 9, 2, 13 }
      stretch           = TRUE
      arrow_direction   = RIGHT
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = INSENSITIVE
      file              = "screw2.png"
      #border           = { 2, 9, 2, 13 }
      stretch           = TRUE
      arrow_direction   = RIGHT
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = NORMAL
      file              = "screw1.png"
      #border           = { 2, 9, 2, 13 }
      stretch           = TRUE
      arrow_direction   = LEFT
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = PRELIGHT
      file              = "screw1.png"
      #border           = { 2, 9, 2, 13 }
      stretch           = TRUE
      arrow_direction   = LEFT
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = ACTIVE
      file              = "screw1.png"
      #border           = { 2, 9, 2, 13 }
      stretch           = TRUE
      arrow_direction   = LEFT
    }
    image
    {
      function          = STEPPER
      recolorable       = FALSE
      state             = INSENSITIVE
      file              = "screw1.png"
      #border           = { 2, 9, 2, 13 }
      stretch           = TRUE
      arrow_direction   = LEFT
    }
  }
}



### NOTEBOOK ###

style "calf-notebook"
{
    xthickness = 24
    ythickness = 8
}



### TREEVIEW ###

style "calf-treeview"
{
    fg[NORMAL]        = "white" # header txt color
    bg[SELECTED]      = "black" # backgr mini-checkbox checked
    base[ACTIVE]      = "DimGray" # selection row unfocused
    base[SELECTED]    = "black" # selection row focused
    text[NORMAL]      = "white" # txt+indicador checked normal
    text[ACTIVE]      = "DeepSkyBlue" # txt+indicador selected unfocused
    text[SELECTED]    = "DeepSkyBlue" # txt+indicador selected focused
}

style "calf-treeview-header" = "calf-treeview"
{
    fg[NORMAL]        = "#fff"
    fg[PRELIGHT]      = "#fff"
    fg[ACTIVE]        = "#fff"
    fg[SELECTED]      = "#0af"
    fg[INSENSITIVE]   = "#aaa"

    bg[NORMAL]        = "#465160"
    bg[PRELIGHT]      = "#000"
    bg[ACTIVE]        = shade (0.9, "#465160")
    bg[SELECTED]      = "#000" # Gnome Appearances Preferences workaround
    bg[INSENSITIVE]   = shade (0.7, "#465160")
}



### FRAMES ###

style "calf-frame"
{
    xthickness = 8
    ythickness = 8
}


widget "*Calf*"                                   style : highest "calf-default"

widget "*Calf-Rack"                               style : highest "calf-rack"
widget "*Calf-Rack*"                              style : highest "calf-rack-widgets"
widget "*Calf-Rack*Calf-Rack-Title"               style : highest "calf-rack-title"

widget "*Calf-Plugin"                             style : highest "calf-plugin"

widget "*Calf-Label"                              style : highest "calf-label"
widget "*Calf-Value"                              style : highest "calf-label"

widget "*Calf-Menu*"                              style : highest "calf-menu"

widget "*Calf*Notebook"                           style : highest "calf-notebook"

widget "*Calf*Frame"                              style : highest "calf-frame"

widget_class "*Calf*TreeView*"                    style : highest "calf-treeview"
widget_class "*Calf*.<GtkTreeView>.<GtkButton>*"  style : highest "calf-treeview-header"
widget_class "*Calf*.<GtkList>.<GtkButton>*"      style : highest "calf-treeview-header"

widget "*Calf*Button*"                            style : highest "calf-button"
widget "*Calf*Toggle*"                            style : highest "calf-toggle"
widget "*Calf-Button"                             style : highest "calf-button"
widget "*Calf-Toggle"                             style : highest "calf-toggle"
widget "*Calf*CheckButton"                        style : highest "calf-toggle"
widget "*Calf*RadioButton"                        style : highest "calf-toggle"

widget "*Calf-Combobox*"                          style : highest "calf-combobox"
widget "*Calf*Combo*"                             style : highest "calf-combobox"
widget_class "*Calf-Combobox*<GtkComboBoxEntry>*" style : highest "calf-comboboxentry"
widget "*Calf*GtkOptionMenu*"                     style : highest "calf-combobox"

widget "*Calf*Entry*"                             style : highest "calf-entry"

widget "*Calf*Range"                              style : highest "calf-scale-small"
widget "*Calf*Scale1"                             style : highest "calf-scale-small"
widget "*Calf*Scale2"                             style : highest "calf-scale-big"
widget "*Calf*Scrollbar*"                         style : highest "calf-scrollbar"
