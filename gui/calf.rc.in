pixmap_path "@guidatadir@"

style "calf-default"
{
    GtkWidget      ::focus-line-width     = 0
    
    GtkRange       ::trough-border        = 0
    GtkScale       ::slider-length        = 46
    GtkRange       ::slider-width         = 35
    GtkScale       ::trough-side-details  = 0
    
    GtkMenuBar     ::internal-padding     = 4
    
    GtkTreeView    ::allow-rules          = 0
    
    GtkArrow       ::arrow-scaling        = 0.0
    GtkComboBox    ::arrow-scaling        = 0.0
    GtkComboBox    ::arrow-size           = 0
    
    xthickness        = 0
    ythickness        = 0
    
    font_name         = "Bitstream Vera Sans 8"
    
    fg[NORMAL]        = "#fff"
    fg[PRELIGHT]      = "#fff"
    fg[ACTIVE]        = "#fff"
    fg[SELECTED]      = "#fff"
    fg[INSENSITIVE]   = "#aaa"

    bg[NORMAL]        = "#222"
    bg[PRELIGHT]      = "#333"
    bg[ACTIVE]        = "#444"
    bg[SELECTED]      = "#0af"
    bg[INSENSITIVE]   = "#666"

    base[NORMAL]      = "#eee"
    base[PRELIGHT]    = "#fff"
    base[ACTIVE]      = "#ddd"
    base[SELECTED]    = "#0af"
    base[INSENSITIVE] = "#666"

    text[NORMAL]      = "#333"
    text[PRELIGHT]    = "#000"
    text[ACTIVE]      = "#000"
    text[SELECTED]    = "#000"
    text[INSENSITIVE] = "#666"
}

style "calf-wide"
{
    xthickness   = 5
    ythickness   = 4
}

style "calf-rack"
{
    bg_pixmap[NORMAL] = "background_light.png"
}

style "calf-rack-title"
{
    fg[NORMAL] = "#000"
    font_name  = "Bitstream Vera Sans Bold 10"
    xthickness = 7
    ythickness = 3
}

style "calf-rack-widgets"
{
    fg[NORMAL]        = "#000"
    fg[PRELIGHT]      = "#000"
    fg[ACTIVE]        = "#000"
    fg[SELECTED]      = "#000"
    fg[INSENSITIVE]   = "#000"
    font_name         = "Bitstream Vera Sans 8"
}

style "calf-plugin"
{
    bg_pixmap[NORMAL] = "background_dark.png"
}

style "calf-label" {
    font_name = "Bitstream Vera Sans 7"
}


### BUTTON ###

style "calf-button"
{
    GtkButton      ::child-displacement-x = 0
    GtkButton      ::child-displacement-y = 0
    GtkButton      ::inner-border         = { 8, 8, 2, 2}
    
    xthickness = 3
    ythickness = 3
    
    fg[NORMAL]          = "#001144"
    fg[PRELIGHT]        = "#115588"
    fg[SELECTED]        = "#001144"
    fg[ACTIVE]          = "#2288aa"
    fg[INSENSITIVE]     = "#aaa"
    
    text[NORMAL]        = "#001144"
    text[PRELIGHT]      = "#115588"
    text[SELECTED]      = "#001144"
    text[ACTIVE]        = "#2288aa"
    text[INSENSITIVE]   = "#aaa"
    
    bg[NORMAL]        = "#DADADA"
    bg[PRELIGHT]      = shade (1.4, "#D1D1D1")
    bg[ACTIVE]        = shade (1.4, "#D1D1D1")
    bg[SELECTED]      = "#DADADA"
    bg[INSENSITIVE]   = shade (0.7, "#D1D1D1")

    base[NORMAL]        = "#DADADA"
    base[PRELIGHT]      = shade (1.4, "#D1D1D1")
    base[ACTIVE]        = shade (1.4, "#D1D1D1")
    base[SELECTED]    = "#DADADA"
    base[INSENSITIVE]   = shade (0.7, "#D1D1D1")
    
    engine "pixmap"
    {
        image
        {
            function        = BOX
            detail          = "buttondefault"
            recolorable     = FALSE
            file            = "button_default.png"
            border          = {6, 6, 6, 6}
            stretch         = TRUE
        }
        image
        {
            function        = BOX
            state           = PRELIGHT
            recolorable     = FALSE
            file            = "button_default_prelight.png"
            border          = {6, 6, 6, 6}
            stretch         = TRUE
        }
        image
        {
            function        = BOX
            state           = ACTIVE
            file            = "button_default_active.png"
            border          = {6, 6, 6, 6}
            stretch         = TRUE
        }
        image
        {
            function        = BOX
            state           = INSENSITIVE
            file            = "button_default.png"
            border          = {6, 6, 6, 6}
            stretch         = TRUE
        }
        image
        {
            function        = BOX
            file            = "button_default.png"
            border          = {6, 6, 6, 6}
            stretch         = TRUE
        }
        
    }
}


### TOGGLE BUTTON ###

style "calf-toggle" = "calf-button"
{
    GtkButton      ::child-displacement-x = 0
    GtkButton      ::child-displacement-y = 0
    GtkButton::inner-border         = { 6, 31, 1, 1}
    
    engine "pixmap"
    {
        image
        {
            function        = BOX
            detail          = "buttondefault"
            recolorable     = FALSE
            file            = "button_normal.png"
            border          = {6, 27, 6, 6}
            stretch         = TRUE
        }
        image
        {
            function        = BOX
            state           = PRELIGHT
            recolorable     = FALSE
            file            = "button_prelight.png"
            border          = {6, 27, 6, 6}
            stretch         = TRUE
        }
        image
        {
            function        = BOX
            state           = ACTIVE
            file            = "button_active.png"
            border          = {6, 27, 6, 6}
            stretch         = TRUE
        }
        image
        {
            function        = BOX
            state           = INSENSITIVE
            file            = "button_insens.png"
            border          = {6, 27, 6, 6}
            stretch         = TRUE
        }
        image
        {
            function        = BOX
            file            = "button_normal.png"
            border          = {6, 27, 6, 6}
            stretch         = TRUE
        }
        
    }
}



### DEFAULT BUTTON ###

style "calf-button-norm" = "calf-button"
{
    GtkButton      ::default-border       = { 7, 25, 5, 5 }
    
    engine "pixmap"
    {
        image
        {
            function        = BOX
            file            = "button_default.png"
            border          = {6, 6, 6, 6}
            stretch         = TRUE
        }
        image
        {
            function        = BOX
            state           = PRELIGHT
            file            = "button_default_prelight.png"
            border          = {6, 6, 6, 6}
            stretch         = TRUE
        }
        image
        {
            function        = BOX
            state           = ACTIVE
            file            = "button_default_active.png"
            border          = {6, 6, 6, 6}
            stretch         = TRUE
        }
        image
        {
            function        = VLINE
            recolorable     = FALSE
            file            = "vline.png"
            border          = { 1, 1, 0, 0 }
            stretch         = TRUE
        }
        image
        {
            function        = HLINE
            recolorable     = FALSE
            file            = "hline.png"
            border          = { 0, 0, 1, 1  }
            stretch         = TRUE
        }
        
    }
}



