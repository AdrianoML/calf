    <hbox>
        <frame label="Input">
            <hbox spacing="10">
                <table expand-y="0" expand-x="1" spacing="5" rows="1" cols="8">
                    <label text="Carrier" attach-x="0" attach-y="0" expand-x="0" />
                    <knob param="carrier_in" type="1" attach-x="0" attach-y="1" attach-h="2" expand-x="0" />
                    <value param="carrier_in" attach-x="0" attach-y="3" expand-x="0" />
                            <label attach-x="1" attach-y="0" expand-x="1" text="Carrier" />
                    <vumeter param="carrier_inL" position="2" mode="0" hold="1.5" falloff="2.5" attach-x="1" attach-y="1" expand-x="1" />
                    <vumeter param="carrier_inR" position="2" mode="0" hold="1.5" falloff="2.5" attach-x="1" attach-y="2" expand-x="1" />
                            <label attach-x="2" attach-y="0" expand-x="0" text="Clip" />
                    <led param="carrier_clip_inL" attach-x="2" attach-y="1" expand-x="0" />
                    <led param="carrier_clip_inR" attach-x="2" attach-y="2" expand-x="0" />
                </table>
                <table expand-y="0" expand-x="1" spacing="5" rows="1" cols="8">
                    <label text="Modulator" attach-x="0" attach-y="0" expand-x="0" />
                    <knob param="mod_in" type="1" attach-x="0" attach-y="1" attach-h="2" expand-x="0" />
                    <value param="mod_in" attach-x="0" attach-y="3" expand-x="0" />
                            <label attach-x="1" attach-y="0" expand-x="1" text="Modulator" />
                    <vumeter param="mod_inL" position="2" mode="0" hold="1.5" falloff="2.5" attach-x="1" attach-y="1" expand-x="1" />
                    <vumeter param="mod_inR" position="2" mode="0" hold="1.5" falloff="2.5" attach-x="1" attach-y="2" expand-x="1" />
                            <label attach-x="2" attach-y="0" expand-x="0" text="Clip" />
                    <led param="mod_clip_inL" attach-x="2" attach-y="1" expand-x="0" />
                    <led param="mod_clip_inR" attach-x="2" attach-y="2" expand-x="0" />
                </table>
            </hbox>
        </frame>
        <frame label="Bands">
            <vbox>
                <knob param="bands_knob" size="3"/>
            </vbox>
        </frame>
        <frame label="Mixer">
            <hbox spacing="10">
                <vbox>
                    <label text="Modulator"/>
                    <knob param="modulator"/>
                    <value param="modulator"/>
                </vbox>
                <vbox>
                    <label text="Carrier"/>
                    <knob param="carrier"/>
                    <value param="carrier"/>
                </vbox>
                <vbox>
                    <label text="Processed"/>
                    <knob param="processed"/>
                    <value param="processed"/>
                </vbox>
            </hbox>
        </frame>
        
        <frame label="Output">
            <table expand-y="0" expand-x="1" spacing="5" rows="1" cols="8">
                <label text="Level" attach-x="0" attach-y="0" expand-x="0" />
                <knob param="out" type="1" attach-x="0" attach-y="1" attach-h="2" expand-x="0" />
                <value param="out" attach-x="0" attach-y="3" expand-x="0" />
                    <label attach-x="1" attach-y="0" expand-x="1" text="Output" />
                <vumeter param="outL" position="2" mode="0" hold="1.5" falloff="2.5" attach-x="1" attach-y="1" expand-x="1" />
                <vumeter param="outR" position="2" mode="0" hold="1.5" falloff="2.5" attach-x="1" attach-y="2" expand-x="1" />
                    <label attach-x="2" attach-y="0" expand-x="0" text="Clip" />
                <led param="clip_outL" attach-x="2" attach-y="1" expand-x="0" />
                <led param="clip_outR" attach-x="2" attach-y="2" expand-x="0" />
            </table>
        </frame>
        
    </hbox>
    
