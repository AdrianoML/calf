<vbox spacing="8">
    <table expand-y="0" expand-x="1" spacing="5" rows="1" cols="8">
        <label param="level_in" attach-x="0" attach-y="0" expand-x="0" />
        <knob param="level_in" type="1" attach-x="0" attach-y="1" attach-h="2" expand-x="0" />
        <value param="level_in" attach-x="0" attach-y="3" expand-x="0" />
        
        <label attach-x="1" attach-y="0" expand-x="1" text="Input level" />
        <vumeter param="meter_inL" position="2" mode="0" hold="1.5" falloff="2.5" attach-x="1" attach-y="1" expand-x="1" />
        <vumeter param="meter_inR" position="2" mode="0" hold="1.5" falloff="2.5" attach-x="1" attach-y="2" expand-x="1" />
        
        <label attach-x="2" attach-y="0" expand-x="0" text="Clip" />
        <led param="clip_inL" attach-x="2" attach-y="1" expand-x="0" />
        <led param="clip_inR" attach-x="2" attach-y="2" expand-x="0" />
        
        <label param="bypass" attach-x="3" attach-y="0" expand-x="0" />
        <toggle param="bypass" attach-x="3" attach-y="1" attach-h="2" expand-x="0" />
        
        <label attach-x="5" attach-y="0" expand-x="1" text="Output level"/>
        <vumeter param="meter_outL" position="2" mode="0" hold="1.5" falloff="2.5" attach-x="5" attach-y="1" expand-x="1" />
        <vumeter param="meter_outR" position="2" mode="0" hold="1.5" falloff="2.5" attach-x="5" attach-y="2" expand-x="1" />
        
        <label attach-x="6" attach-y="0" expand-x="0" text="Clip"/>
        <led param="clip_outL" mode="1" attach-x="6" attach-y="1" expand-x="0" />
        <led param="clip_outR" mode="1" attach-x="6" attach-y="2" expand-x="0" />
        
        <label param="level_out" attach-x="7" attach-y="0" expand-x="0" />
        <knob param="level_out" type="1" attach-x="7" attach-y="1" attach-h="2" expand-x="0" />
        <value param="level_out" attach-x="7" attach-y="3" expand-x="0" />
    </table>
    
    <hbox>
        <frame label="LFO 1">
            <table rows="5" cols="4">
                <label text="Modulator Frequency" attach-x="0" attach-y="0" attach-w="3"/>
                <vbox attach-x="0" attach-y="1">
                    <label text="Active"/>
                    <toggle param="lfo1_mod_freq_active" size="1"/>
                    <label/>
                </vbox>
                <vbox attach-x="1" attach-y="1">
                    <label text="Min"/>
                    <knob param="lfo1_mod_freq_lo" size="2"/>
                    <value param="lfo1_mod_freq_lo"/>
                </vbox>
                <vbox attach-x="2" attach-y="1">
                    <label text="Max"/>
                    <knob param="lfo1_mod_freq_hi" size="2"/>
                    <value param="lfo1_mod_freq_hi"/>
                </vbox>
                
                <label text="Modulator Detune" attach-x="0" attach-y="2" attach-w="3"/>
                <vbox attach-x="0" attach-y="3">
                    <label text="Active"/>
                    <toggle param="lfo1_mod_detune_active" size="1"/>
                    <label/>
                </vbox>
                <vbox attach-x="1" attach-y="3">
                    <label text="Min"/>
                    <knob param="lfo1_mod_detune_lo" size="2"/>
                    <value param="lfo1_mod_detune_lo"/>
                </vbox>
                <vbox attach-x="2" attach-y="3">
                    <label text="Max"/>
                    <knob param="lfo1_mod_detune_hi" size="2"/>
                    <value param="lfo1_mod_detune_hi"/>
                </vbox>
                
                <align attach-x="3" attach-y="0" attach-h="4"><vbox spacing="19">
                    <hbox spacing="3"><label text="Frequency"/><align><led param="lfo2_lfo1_freq_active" mode="1" size="0" expand="0" fill="0"/></align></hbox>
                    <knob param="lfo1_freq" size="4"/>
                    <value param="lfo1_freq"/>
                </vbox></align>
                
                <combo param="lfo1_mode" attach-x="1" attach-y="4" attach-w="2"/>
                <button param="lfo1_reset" attach-x="3" attach-y="4"/>
                <led param="lfo1_activity" mode="4" attach-x="0" attach-y="4"/>
                
            </table>
            
        </frame>
        
        <frame label="Modulator">
            
            <table rows="4" cols="2">
                <label attach-x="0" attach-y="0"/>
                <vbox attach-x="0" attach-y="1">
                    <hbox spacing="3"><label text="Detune"/><align><led param="lfo1_mod_detune_active" mode="1" size="0" expand="0" fill="0"/></align></hbox>
                    <knob param="mod_detune" size="2" type="1"/>
                    <value param="mod_detune"/>
                </vbox>
                <vbox attach-x="0" attach-y="2">
                    <label text="Phase"/>
                    <knob param="mod_phase" size="2"/>
                    <value param="mod_phase"/>
                </vbox>
                
                <vbox attach-x="1" attach-y="1" attach-h="2" spacing="10">
                    <hbox spacing="3"><label text="Frequency"/><align><led param="lfo1_mod_freq_active" mode="1" size="0" expand="0" fill="0"/></align></hbox>
                    <knob param="mod_freq" size="5"/>
                    <value param="mod_freq"/>
                </vbox>
                
                <vbox attach-x="2" attach-y="1">
                    <hbox spacing="3"><label text="Amount"/><align><led param="lfo2_mod_amount_active" mode="1" size="0" expand="0" fill="0"/></align></hbox>
                    <knob param="mod_amount" size="2"/>
                    <value param="mod_amount"/>
                </vbox>
                <vbox attach-x="2" attach-y="2">
                    <label text="Listen"/>
                    <toggle param="mod_listen"/>
                    <label/>
                </vbox>
                <vbox attach-x="0" attach-y="3" attach-w="3">
                    <label text="Waveform"/>
                    <combo param="mod_mode"/>
                </vbox>
            </table>
            
        </frame>
        
        <frame label="LFO 2">
            
            <table rows="5" cols="4">
                <label text="LFO 1 Frequency" attach-x="1" attach-y="0" attach-w="3"/>
                <vbox attach-x="1" attach-y="1">
                    <label text="Min"/>
                    <knob param="lfo2_lfo1_freq_lo" size="2"/>
                    <value param="lfo2_lfo1_freq_lo"/>
                </vbox>
                <vbox attach-x="2" attach-y="1">
                    <label text="Max"/>
                    <knob param="lfo2_lfo1_freq_hi" size="2"/>
                    <value param="lfo2_lfo1_freq_hi"/>
                </vbox>
                <vbox attach-x="3" attach-y="1">
                    <label text="Active"/>
                    <toggle param="lfo2_lfo1_freq_active" size="1"/>
                    <label/>
                </vbox>
                
                <label text="Modulator Amount" attach-x="1" attach-y="2" attach-w="3"/>
                <vbox attach-x="1" attach-y="3">
                    <label text="Min"/>
                    <knob param="lfo2_mod_amount_lo" size="2"/>
                    <value param="lfo2_mod_amount_lo"/>
                </vbox>
                <vbox attach-x="2" attach-y="3">
                    <label text="Max"/>
                    <knob param="lfo2_mod_amount_hi" size="2"/>
                    <value param="lfo2_mod_amount_hi"/>
                </vbox>
                <vbox attach-x="3" attach-y="3">
                    <label text="Active"/>
                    <toggle param="lfo2_mod_amount_active" size="1"/>
                    <label/>
                </vbox>
                
                <align attach-x="0" attach-y="0" attach-h="4"><vbox spacing="19">
                    <label/>
                    <label text="Frequency"/>
                    <knob param="lfo2_freq" size="4"/>
                    <value param="lfo2_freq"/>
                    <label/>
                </vbox></align>
                
                <button param="lfo2_reset" attach-x="0" attach-y="4"/>
                <combo param="lfo2_mode" attach-x="1" attach-y="4" attach-w="2"/>
                <led param="lfo2_activity" mode="4" attach-x="3" attach-y="4"/>
            </table>
        </frame>
        
    </hbox>
</vbox>
